@using nTestSwarm.Application.Domain
@model UserAgent

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>UserAgent</legend>

    <div class="display-label">Created</div>
    <div class="display-field">@String.Format("{0:g}", Model.Created)</div>

    <div class="display-label">Updated</div>
    <div class="display-field">@String.Format("{0:g}", Model.Updated)</div>

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">Browser</div>
    <div class="display-field">@Model.Browser</div>

    <div class="display-label">Version</div>
    <div class="display-field">@Model.Version</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


