
@{Layout = "~/Views/Shared/_Layout.cshtml";}

@{ Html.BeginForm("DescribeNew", "Job", new {area = "Api"}, FormMethod.Post, null); }

<p>@Html.Label("url", "URL: ")
@Html.TextBox("url")</p>

<p>@Html.Label("correlation", "Correlation: ")
@Html.TextBox("correlation")</p>

<p>@Html.Label("maxruns", "Max Runs: ")
    @Html.TextBox("maxruns")</p>
    
<p>@Html.Label("apikey", "Api Key: ")
    @Html.TextBox("apikiey")</p>

<p>@Html.Label("private", "Private: ")
    @Html.CheckBox("private")</p>

<input type="submit" value="submit" />

@{Html.EndForm();}



